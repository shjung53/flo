            android:textColor="#BABABA"
            android:textSize="13sp"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@id/home_album_exp1_cv"
            app:layout_constraintBottom_toBottomOf="@id/home_album_exp1_cv"
            app:layout_constraintTop_toBottomOf="@id/home_album_exp1_song_tv"
            />
        <TextView
